.app{
  width: 750rpx;
  overflow: hidden;
}
.userMsg{
  margin: 30rpx 30rpx 0 30rpx;
  padding-bottom: 100rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-bottom: 1px solid #f3f3f3;
}
.userMsg_userImg{
  width: 173rpx;
  height: 173rpx;
  border: 1px solid #000;
  border-radius: 50%;
  margin: 50rpx;
}
.userMsg_username{
  font-size: x-large;
  font-weight: 700;
}
.userMsg_userTel{
  font-size: 14px;
  color: #666666;
}
.menu_list{
  display: flex;
  margin: 0 30rpx;
  padding: 40rpx 20rpx;
  border-bottom: 1px solid #f3f3f3;
  align-items: center;
  justify-content: space-between;
}
.arrows{
  width: 48rpx;
  height: 48rpx;
}